@using Konan.Models
@model IEnumerable<Solution>

<table class="table table-striped table-hover table-responsive">
    <thead>
        <tr>
            <th>Date</th>
            <th>OP</th>
            <th>Language</th>
            <th>Input</th>
            <th>Output</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Date</td>
                <td><a href=@Url.Action("ViewProfile", "Account", new { id = @item.Account.Id})> @item.Account.FirstName&nbsp;@item.Account.LastName</a></td>
                <td>@item.Language</td>
                <td>@item.Post.Input</td>
                <td>@item.Output</td>
                <td><a onclick="redirect('/Problems/Solution/' + '@item.Id')"><span class="glyphicon glyphicon-eye-open"></span></a></td>
            </tr>
        }
    </tbody>
</table>